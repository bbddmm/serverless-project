{
  "name": "serverless-project",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "dev": "serverless offline",
    "prepare": "husky install",
    "preinstall": "typesync || :",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json,md,yml,yaml}'",
    "fix": "run-s format fix:es fix:style",
    "fix:es": "eslint --fix './**/*.{js,jsx,ts,tsx}'",
    "lint": "run-p lint:*",
    "lint:es": "eslint './**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier --check './**/*.{js,jsx,ts,tsx,json,md,yml,yaml}'",
    "lint:conflict": "eslint-config-prettier './**/*.{js,jsx,ts,tsx,vue}'"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^3.6.2",
    "@middy/http-json-body-parser": "^3.6.2",
    "@vendia/serverless-express": "^4.10.1",
    "body-parser": "^1.20.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@serverless/typescript": "^3.21.0",
    "@tsconfig/strictest": "^1.0.2",
    "@types/aws-lambda": "^8.10.108",
    "@types/body-parser": "^1.19.2",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.14",
    "@types/node": "^16.18.1",
    "@types/prettier": "^2.7.1",
    "@types/serverless": "^3.12.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "esbuild": "^0.15.15",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "json-schema-to-ts": "^2.6.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "serverless": "^3.25.0",
    "serverless-esbuild": "^1.33.2",
    "serverless-offline": "^11.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "typesync": "^0.9.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
